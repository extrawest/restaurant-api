{
	"name": "nx-restaurant",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"start-server": "nx serve restaurant-server",
		"start-prod": "node main",
		"lint": "nx workspace-lint && nx lint",
		"format": "nx format:write --uncommitted --parallel --maxParallel=5 --skip-nx-cache",
		"lint:all": "nx run-many --target=lint --all --maxWarnings=-1 --skip-nx-cache",
		"lint:all:fix": "nx run-many --target=lint --all --fix --maxWarnings=-1 --skip-nx-cache",
		"test:restaurant-api": "nx test restaurant-api",
		"prepare": "husky install",
		"build": "nx build restaurant-server --skip-nx-cache --prod",
		"migration-up": "node umzug.mjs up",
		"migration-down": "node umzug.mjs down"
	},
	"jest": {
    "collectCoverage": true,
    "coverageReporters": ["json", "html"]
  },
	"private": true,
	"dependencies": {
		"@casl/ability": "^6.5.0",
		"@extrawest/design-system": "^0.0.1",
		"@nestjs/common": "10.3.0",
		"@nestjs/config": "^3.1.1",
		"@nestjs/core": "10.3.0",
		"@nestjs/jwt": "^10.2.0",
		"@nestjs/platform-express": "10.3.0",
		"@nestjs/sequelize": "^10.0.1",
		"@nestjs/swagger": "7.2.0",
		"@types/amqplib": "^0.10.4",
		"amqp-connection-manager": "^4.1.14",
		"amqplib": "^0.10.3",
		"axios": "^1.0.0",
		"bcrypt": "^5.1.1",
		"dotenv": "^16.3.1",
		"firebase-admin": "^12.0.0",
		"lodash": "^4.17.21",
		"nodemailer": "^6.9.8",
		"passport-jwt": "^4.0.1",
		"pg": "^8.11.3",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.8.0",
		"sequelize": "^6.35.2",
		"sequelize-typescript": "^2.1.6",
		"tslib": "^2.3.0",
		"umzug": "^3.5.0"
	},
	"devDependencies": {
		"@nestjs/schematics": "10.1.0",
		"@nestjs/testing": "10.3.0",
		"@nrwl/eslint-plugin-nx": "17.2.8",
		"@nrwl/jest": "17.2.8",
		"@nrwl/js": "17.2.8",
		"@nrwl/linter": "17.2.8",
		"@nrwl/nest": "17.2.8",
		"@nrwl/node": "17.2.8",
		"@nrwl/nx-cloud": "16.5.2",
		"@nrwl/webpack": "17.2.8",
		"@nrwl/workspace": "17.2.8",
		"@nx/eslint-plugin": "17.2.8",
		"@nx/jest": "17.2.8",
		"@nx/js": "17.2.8",
		"@nx/nest": "17.2.8",
		"@nx/node": "17.2.8",
		"@nx/webpack": "17.2.8",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
		"@svgr/webpack": "8.1.0",
		"@swc-node/register": "1.6.8",
		"@swc/core": "1.3.103",
		"@types/bcrypt": "^5.0.2",
		"@types/jest": "^29.4.0",
		"@types/multer": "^1.4.11",
		"@types/node": "18.16.9",
		"@types/nodemailer": "^6.4.14",
		"@types/pg": "^8.11.0",
		"@types/sequelize": "^4.28.19",
		"@typescript-eslint/eslint-plugin": "6.19.0",
		"@typescript-eslint/parser": "6.19.0",
		"eslint": "8.48.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"husky": "^8.0.0",
		"jest": "^29.4.1",
		"jest-environment-node": "^29.4.1",
		"lint-staged": "^15.2.0",
		"nx": "17.2.8",
		"prettier": "3.2.2",
		"react-refresh": "^0.10.0",
		"ts-jest": "29.1.1",
		"ts-node": "10.9.1",
		"typescript": "5.2.2",
		"url-loader": "^4.1.1"
	},
	"lint-staged": {
		"{apps,libs}/**/*.{ts,tsx}": [
			"yarn lint:all"
		],
		"*.json": [
			"prettier --write"
		]
	}
}
