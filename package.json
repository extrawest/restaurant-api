{
	"name": "nx-restaurant",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"start-server": "nx serve restaurant-server",
		"start-client": "nx dev restaurant-client -p 3002",
		"start-prod": "node main",
		"lint": "nx workspace-lint && nx lint",
		"format": "nx format:write --uncommitted --parallel --maxParallel=5 --skip-nx-cache",
		"lint:all": "nx run-many --target=lint --all --maxWarnings=-1 --skip-nx-cache",
		"lint:all:fix": "nx run-many --target=lint --all --fix --maxWarnings=-1 --skip-nx-cache",
		"test:restaurant-api": "nx test restaurant-api",
		"test:coverage": "nx test restaurant-api --code-coverage",
		"prepare": "husky install",
		"build": "nx build restaurant-server --skip-nx-cache --prod",
		"build-client": "nx build restaurant-client --skip-nx-cache --prod",
		"migration-up": "node umzug.mjs up",
		"migration-down": "node umzug.mjs down"
	},
	"private": true,
	"dependencies": {
		"@casl/ability": "^6.5.0",
		"@extrawest/design-system": "^0.0.1",
		"@mui/icons-material": "^5.15.15",
