		"@svgr/webpack": "8.1.0",
		"@swc-node/register": "~1.8.0",
		"@swc/cli": "~0.1.62",
		"@swc/core": "1.3.103",
		"@swc/helpers": "~0.5.2",
		"@testing-library/react": "14.0.0",
		"@types/bcrypt": "^5.0.2",
