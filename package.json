		"sequelize": "^6.35.2",
		"sequelize-typescript": "^2.1.6",
		"socket.io-client": "^4.7.4",
		"stripe": "^14.18.0",
		"tslib": "^2.3.0",
		"umzug": "^3.5.0"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@nestjs/schematics": "10.1.0",
		"@nestjs/testing": "10.3.0",
		"@nrwl/eslint-plugin-nx": "18.3.0",
		"@nrwl/jest": "18.3.0",
		"@nrwl/js": "18.3.0",
		"@nrwl/linter": "18.3.0",
		"@nrwl/nest": "18.3.0",
		"@nrwl/node": "18.3.0",
		"@nrwl/nx-cloud": "18.0.0",
		"@nrwl/webpack": "18.3.0",
